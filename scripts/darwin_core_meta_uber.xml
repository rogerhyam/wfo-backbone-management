            <location>classification.csv</location>
        </files>

        <!-- record id is guid -->
        <id index="0" />

        <!-- column mappings -->
        <field index="0" term="http://rs.tdwg.org/dwc/terms/taxonID"/>
        <field index="1" term="http://rs.tdwg.org/dwc/terms/scientificNameID"/>
        <field index="2" term="http://worldfloraonline/terms/localID"/>
